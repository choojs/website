    "tachyons": "^4.5.6",
    "vhs": "^0.2.0"
  },
  "devDependencies": {
    "bankai": "^5.1.3",
    "brfs": "^1.4.3",
    "dependency-check": "^2.6.0",
    "standard": "^8.6.0",
    "surge": "^0.18.0"
  }
}
